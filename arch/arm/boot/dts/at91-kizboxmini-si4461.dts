/dts-v1/;
#include "at91-kizboxmini_common.dtsi"

/ {
	model = "Overkiz Kizbox mini with SI4461";
	compatible = "overkiz,kizboxmini-si4461", "atmel,at91sam9g25", "atmel,at91sam9x5", "atmel,at91sam9";

	ahb {
		apb {
			usart3: serial@f8028000 {
				status = "okay";
			};

			spi1: spi@f0004000 {
				status = "okay";
			};
		};
	};
};
